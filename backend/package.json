{
  "name": "fizzbuzz-eaten-backend",
  "version": "1.0.0",
  "description": "Food analysis API server with AI integration and MySQL database",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:init": "node src/database/init.js",
    "db:migrate": "node -e \"require('./src/database/init').runMigrations()\"",
    "db:seed": "node -e \"require('./src/database/init').runSeeds()\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "form-data": "^4.0.0",
    "mysql2": "^3.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
